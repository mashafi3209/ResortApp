<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
  minWidth="-Infinity" prefHeight="1090.0" prefWidth="1226.0"
  vbarPolicy="ALWAYS" xmlns="http://javafx.com/javafx/8.0.171"
  fx:controller="sample.DeskAssistantController">
  <content>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="1116.0" prefWidth="1600.0">
      <children>
        <TabPane maxHeight="-Infinity" prefHeight="1014.0" prefWidth="1541.0"
          tabClosingPolicy="UNAVAILABLE">
          <tabs>
            <Tab text="Booking History">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>
                    <Button layoutX="771.0" layoutY="213.0" mnemonicParsing="false"
                      onAction="#acceptPaymentAction" text="Accept Payment"
                      fx:id="acceptPayment"/>
                    <Button layoutX="771.0" layoutY="264.0" mnemonicParsing="false"
                      onAction="#checkinCustomerAction" text="Checkin Customer"
                      fx:id="checkinCustomer"/>
                    <Button layoutX="1035.0" layoutY="10.0" mnemonicParsing="false"
                      onAction="#logOut" text="Logout" fx:id="logoutButton"/>
                    <Label layoutX="897.0" layoutY="14.0" text="Null" fx:id="idSpace1"/>
                    <Label layoutX="802.0" layoutY="14.0" text="Logged in as:"/>
                    <TableView layoutX="14.0" layoutY="7.0" prefHeight="583.0" prefWidth="736.0"
                      fx:id="tableActivity">
                      <columns>
                        <TableColumn prefWidth="77.00003051757812" text="Invoice No"
                          fx:id="invoiceID">
                          <cellValueFactory>
                            <PropertyValueFactory property="invoice"/>
                          </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="121.0" text="Full Name" fx:id="fullNametoDisplay">
                          <cellValueFactory>
                            <PropertyValueFactory property="cname"/>
                          </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="91.0" text="Room Number" fx:id="roomNo">
                          <cellValueFactory>
                            <PropertyValueFactory property="room"/>
                          </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="164.0" text="Stay Date" fx:id="dateToDisplay">
                          <cellValueFactory>
                            <PropertyValueFactory property="date"/>
                          </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="92.0" text="Amount" fx:id="amount">
                          <cellValueFactory>
                            <PropertyValueFactory property="cost"/>
                          </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="103.0" text="Payment Status" fx:id="paymentStatus">
                          <cellValueFactory>
                            <PropertyValueFactory property="paid"/>
                          </cellValueFactory>
                        </TableColumn>
                        <TableColumn prefWidth="87.0" text="CheckinStatus" fx:id="checkInStatus">
                          <cellValueFactory>
                            <PropertyValueFactory property="checkinStatus"/>
                          </cellValueFactory>
                        </TableColumn>
                      </columns>
                    </TableView>
                  </children>
                </AnchorPane>
              </content>
            </Tab>
          </tabs>
        </TabPane>
      </children>
    </AnchorPane>
  </content>
</ScrollPane>
